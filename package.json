{
   "name": "my-app",
   "version": "0.1.0",
   "private": true,
   "dependencies": {
      "@emotion/react": "^11.8.1",
      "@emotion/styled": "^11.8.1",
      "@mui/icons-material": "^5.4.2",
      "@mui/material": "^5.4.2",
      "@testing-library/jest-dom": "^5.16.2",
      "@testing-library/react": "^12.1.2",
      "@testing-library/user-event": "^13.5.0",
      "@types/jest": "^27.4.0",
      "@types/node": "^16.11.22",
      "@types/react": "^17.0.39",
      "@types/react-dom": "^17.0.11",
      "absolute-version": "^1.0.1",
      "axios": "^0.26.0",
      "babel-jest": "^27.5.1",
      "babel-plugin-istanbul": "^6.1.1",
      "cypress-react-selector": "^3.0.0",
      "eslint-plugin-simple-import-sort": "^7.0.0",
      "html-react-parser": "^3.0.1",
      "instead": "^1.0.3",
      "istanbul-lib-coverage": "^3.2.0",
      "npm": "^8.12.0",
      "nyc": "^15.1.0",
      "outdated": "^0.1.1",
      "react": "^17.0.2",
      "react-chartjs-2": "^4.0.1",
      "react-dom": "^17.0.2",
      "react-hook-form": "^7.28.0",
      "react-intl": "^6.0.4",
      "react-query": "^3.34.16",
      "react-router-dom": "^6.2.1",
      "react-scripts": "5.0.0",
      "react-scroll": "^1.8.6",
      "react-toastify": "^8.2.0",
      "sass": "^1.49.7",
      "ts-jest": "^27.1.4",
      "typescript": "^4.5.5",
      "usehooks-ts": "^2.4.2",
      "web-vitals": "^2.1.4"
   },
   "scripts": {
      "start": "react-scripts -r @cypress/instrument-cra start && eslint src/* ",
      "build": "react-scripts build",
      "dummy-server": "npx json-server --watch db.json --port 3004 --delay 2000",
      "eject": "react-scripts eject",
      "cy:tests-e2e": "cypress open",
      "cy:tests-unit": "npx cypress open-ct",
      "cy:tests-e2e-cl": "cypress run",
      "cy:tests-unit-cl": "npx cypress run-ct",
      "cy:tests-import": "npx cypress run-ct --spec \"cypress/integration/unit-tests/Footer.test.js\"",
      "cy:coverage-unit": "npx nyc cypress run-ct",
      "cy:coverage-e2e": "npx nyc cypress run",
      "cy:coverage": "npx nyc cypress run && npx nyc cypress run-ct",
      "cy:report": "open coverage/lcov-report/index.html",
      "eslint:test": "npx eslint src/**",
      "eslint:fix": "npx eslint --fix . --ext .tsx,.ts src"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "@bahmutov/cypress-code-coverage": "^1.3.1",
      "@cypress/code-coverage": "^3.10.0-dev.1",
      "@cypress/instrument-cra": "^1.4.0",
      "@cypress/react": "^5.10.0",
      "@cypress/webpack-dev-server": "^2.0.0",
      "@types/react-scroll": "^1.8.3",
      "@typescript-eslint/eslint-plugin": "^5.12.0",
      "@typescript-eslint/parser": "^5.12.0",
      "cypress": "^10.3.1",
      "cypress-mochawesome-reporter": "^3.2.0",
      "cypress-multi-reporters": "^1.6.1",
      "eslint": "^8.9.0",
      "eslint-plugin-cypress": "^2.12.1",
      "eslint-plugin-react": "^7.28.0",
      "html-webpack-plugin": "^4.5.2",
      "junit-report-merger": "^3.0.5",
      "mocha": "^10.0.0",
      "mocha-junit-reporter": "^2.0.2"
   },
   "nyc": {
      "all": false,
      "excludeAfterRemap": true,
      "extension": [
         ".jsx",
         ".js",
         ".ts",
         ".tsx"
      ],
      "exclude": [
         "cypress/**/*.js"
      ],
      "reporter": [
         "lcov",
         "json-summary",
         "text",
         "cobertura",
         "text-summary"
      ]
   }
}
